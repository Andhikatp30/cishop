{
    "version": 2,
    "builds": [
      {
        "src": "index.php",
        "use": "@vercel/php",
        "config": {
          "postInstall": "apt-get update && apt-get install -y libssl-dev"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }
  